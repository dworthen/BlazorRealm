{
  "name": "docs",
  "version": "1.0.0",
  "description": "Docs for Blazor Realm",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "preinstall:docfx": "rimraf docfx/*",
    "install:docfx": "nuget install docfx.console -Version 2.36.2 -OutputDirectory docfx",
    "postinstall:docfx": "copyfiles docfx/docfx.console*/**/* docfx -u 2 && rimraf docfx/docfx.console*",
    "install": "npm-run-all install:docfx",
    "clean:quickstart": "rimraf docs/quickstart.md",
    "clean:gh-pages": "rimraf ../gh-pages/*",
    "move:readme": "copyfiles ../README.md docs/docs && replace \"docs/\" \"../\" docs/README.md && renamer -f README.md -r quickstart.md docs/*",
    "move:gh-pages": "copyfiles _site/**/* ../gh-pages -u 1",
    "build:metadata": "docfx\\tools\\docfx.exe metadata docfx.json",
    "build:docs": "docfx\\tools\\docfx.exe build docfx.json",
    "prebuild:docs": "npm-run-all clean:quickstart move:readme build:metadata",
    "postbuild:docs": "npm-run-all clean:gh-pages move:gh-pages",
    "build": "npm-run-all build:docs",
    "serve:docs": "docfx\\tools\\docfx.exe serve _site\\ -p 9000"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "copyfiles": "^2.0.0",
    "npm-run-all": "^4.1.3",
    "renamer": "^0.7.3",
    "replace": "^1.0.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {}
}
